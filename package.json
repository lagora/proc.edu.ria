{
  "name": "proc.edu.ria",
  "version": "1.0.0",
  "description": "a procedural city",
  "main": "index.html",
  "scripts": {
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --watch",

    "gen-all": "node dist/generator.js --dump-db --dump-file --size 4",
    "gen-db": "node dist/generator.js --dump-db --size 4",
    "gen-file": "node dist/generator.js --dump-file --size 8",
    "watch-gen": "webpack --config webpack.config.generator.js --watch",
    "build-gen": "webpack --config webpack.config.client.js",

    "arch": "node dist/archiver.js",
    "watch-arch": "webpack --config webpack.config.archiver.js --watch",

    "watch-server": "webpack --config webpack.config.server.js --watch",
    "build-server": "webpack --config webpack.config.server.js",

    "watch-client": "webpack --config webpack.config.client.js --watch",
    "build-client": "webpack --config webpack.config.client.js",

    "build": "npm run build-gen && npm run build-server && npm run build-client",

    "front": "http-server -p 8081",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-lagora:lagora/proc.edu.ria.git"
  },
  "author": "franck @lagora",
  "license": "MIT",
  "dependencies": {
    "async-waterfall": "^0.1.5",
    "babel": "^6.5.2",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-webpack": "^6.4.0",
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.23.1",
    "fs-extra": "^0.26.7",
    "rethinkdb": "^2.3.0",
    "fs-extra": "^0.26.7",
    "html-loader": "^0.4.3",
    "json-loader": "^0.5.4",
    "rethinkdbdash": "^2.2.18",
    "sha512": "0.0.1",
    "three-orbit-controls": "^72.0.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-watch": "^4.3.5",
    "style-loader": "^0.13.1",
    "three": "^0.75.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
  }
}
