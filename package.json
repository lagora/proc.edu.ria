{
  "name": "proc.edu.ria",
  "version": "1.0.0",
  "description": "a procedural city",
  "main": "index.html",
  "scripts": {
    "start": "node server/dist/proc.edu.ria.server.js",
    "gen-all": "node dist/generator.js --dump-db --dump-file --size 4",
    "gen-db": "node dist/generator.js --dump-db --size 4",
    "gen-file-4": "node dist/generator.js --dump-file --size 4",
    "gen-file-8": "node dist/generator.js --dump-file --size 8",
    "gen-file-16": "node dist/generator.js --dump-file --size 16",
    "gen-file": "npm run gen-file-4",
    "gen-file-all": "npm run gen-file-4 && npm run gen-file-8 && npm run gen-file-16",
    "watch-gen": "./node_modules/webpack/bin/webpack.js --config webpack.config.generator.js --watch",
    "build-gen": "./node_modules/webpack/bin/webpack.js --config webpack.config.generator.js",
    "arch": "node dist/archiver.js",
    "watch-arch": "./node_modules/webpack/bin/webpack.js --config webpack.config.archiver.js --watch",
    "watch-server": "./node_modules/webpack/bin/webpack.js --config webpack.config.server.js --watch",
    "build-server": "./node_modules/webpack/bin/webpack.js --config webpack.config.server.js",
    "watch-client": "./node_modules/webpack/bin/webpack.js --config webpack.config.client.js --watch",
    "build-client": "./node_modules/webpack/bin/webpack.js --config webpack.config.client.js",
    "build": "npm run build-gen && npm run build-server && npm run build-client",
    "front": "http-server -p 8081",
    "postinstall": "npm run build && npm run gen-file-all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-lagora:lagora/proc.edu.ria.git"
  },
  "author": "franck @lagora",
  "license": "MIT",
  "dependencies": {
    "async-waterfall": "^0.1.5",
    "babel": "^5.8.38",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-webpack": "^6.4.0",
    "babel-runtime": "^6.6.1",
    "connect": "^3.4.1",
    "css-loader": "^0.23.1",
    "engine.io": "^1.6.8",
    "fs-extra": "^0.26.7",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-watch": "^4.3.5",
    "html-loader": "^0.4.3",
    "json-loader": "^0.5.4",
    "rethinkdb": "^2.3.0",
    "rethinkdbdash": "^2.2.18",
    "serve-static": "^1.10.2",
    "sha512": "0.0.1",
    "socket.io": "^1.4.5",
    "style-loader": "^0.13.1",
    "three": "^0.75.0",
    "three-orbit-controls": "^72.0.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {}
}
